{
  "name": "polymer-weight",
  "version": "1.0.0-beta.6",
  "description": "size of imported components and scripts",
  "main": "index.js",
  "bin": {
    "weigh": "lib/cli.js"
  },
  "scripts": {
    "compile": "npm run clean && babel -d lib/ src/ --presets=es2015,stage-0 --require babel-polyfill",
    "clean": "rm -rf lib/",
    "prepublish": "npm run compile",
    "test": "npm run compile && jest"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "src/",
      "node_modules/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aruntk/polymer-weight.git"
  },
  "keywords": [
    "polymer",
    "weight",
    "size",
    "webcomponents",
    "html",
    "link",
    "import",
    "script",
    "style"
  ],
  "author": "aruntk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aruntk/polymer-weight/issues"
  },
  "homepage": "https://github.com/aruntk/polymer-weight#readme",
  "dependencies": {
    "babel-polyfill": "^6.22.0",
    "cli-table": "^0.3.1",
    "filesize": "^3.5.4",
    "lodash": "^4.17.4",
    "meow": "^3.7.0",
    "parse5": "^3.0.1",
    "polymer-analyzer": "^2.0.0-alpha.19"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.22.0",
    "eslint": "^3.14.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "jest": "^18.1.0"
  }
}
